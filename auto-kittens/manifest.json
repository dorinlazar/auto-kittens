{
  "name": "Auto kittens",
  "description": "Kittens automation",
  "version": "0.1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/cat128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "images/cat16.png",
    "48": "images/cat48.png",
    "128": "images/cat128.png"
  },
  "permissions": [
    "*://bloodrizer.ru/games/kittens/"
  ],
  "short_name": "Auto.Kittens"
}